import java.util.*;

public class FibonacciHeap {
    private Node minNode;
    private int count;
    
    private class Node {
        int key;
        int degree;
        boolean marked;
        Node parent;
        Node child;
        Node left;
        Node right;
        
        Node(int key) {
            this.key = key;
            this.left = this;
            this.right = this;
        }
    }
    
    public FibonacciHeap() {
        minNode = null;
        count = 0;
    }
    
    public void insert(int key) {
        Node node = new Node(key);
        if (minNode == null) {
            minNode = node;
        } else {
            addToRootList(node);
            if (key < minNode.key) {
                minNode = node;
            }
        }
        count++;
    }
    
    private void addToRootList(Node node) {
        node.left = minNode;
        node.right = minNode.right;
        minNode.right.left = node;
        minNode.right = node;
    }
    
    public Integer findMin() {
        return minNode != null ? minNode.key : null;
    }
    
    public Integer extractMin() {
        Node z = minNode;
        if (z != null) {
            if (z.child != null) {
                Node child = z.child;
                do {
                    Node nextChild = child.right;
                    addToRootList(child);
                    child.parent = null;
                    child = nextChild;
                } while (child != z.child);
            }
            
            z.left.right = z.right;
            z.right.left = z.left;
            
            if (z == z.right) {
                minNode = null;
            } else {
                minNode = z.right;
                consolidate();
            }
            
            count--;
        }
        return z != null ? z.key : null;
    }
    
    private void consolidate() {
        int maxDegree = (int) Math.floor(Math.log(count) / Math.log(2)) + 1;
        Node[] degreeTable = new Node[maxDegree + 1];
        
        List<Node> nodes = new ArrayList<>();
        Node current = minNode;
        do {
            nodes.add(current);
            current = current.right;
        } while (current != minNode);
        
        for (Node node : nodes) {
            Node x = node;
            int d = x.degree;
            while (degreeTable[d] != null) {
                Node y = degreeTable[d];
                if (x.key > y.key) {
                    Node temp = x;
                    x = y;
                    y = temp;
                }
                heapLink(y, x);
                degreeTable[d] = null;
                d++;
            }
            degreeTable[d] = x;
        }
        
        minNode = null;
        for (Node node : degreeTable) {
            if (node != null) {
                if (minNode == null) {
                    minNode = node;
                } else if (node.key < minNode.key) {
                    minNode = node;
                }
            }
        }
    }
    
    private void heapLink(Node y, Node x) {
        y.left.right = y.right;
        y.right.left = y.left;
        
        y.parent = x;
        if (x.child == null) {
            x.child = y;
            y.left = y;
            y.right = y;
        } else {
            y.left = x.child;
            y.right = x.child.right;
            x.child.right.left = y;
            x.child.right = y;
        }
        
        x.degree++;
        y.marked = false;
    }
}
